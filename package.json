{
  "name": "r-factor",
  "version": "0.0.1",
  "description": "React refactoring tools for Sublime 3",
  "main": "index.js",
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC BY-NC-ND 4.0",
  "scripts": {
    "lint": "eslint config . --ext .js",
    "lint:fix": "eslint config . --ext .js --fix",
    "test": "jest --no-cache --coverage",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --no-cache --watch",
    "test:debug:windows": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache --watch",
    "test:watch": "jest --no-cache --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ]
  },
  "dependencies": {
    "@babel/generator": "7.0.0-beta.49",
    "@babel/parser": "7.0.0-beta.48",
    "@babel/template": "7.0.0-beta.49",
    "@babel/traverse": "7.0.0-beta.49",
    "@babel/types": "7.0.0-beta.49",
    "get-stdin": "6.0.0",
    "lodash.difference": "4.5.0",
    "stable": "0.1.8",
    "yargs": "11.1.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "jest": "23.1.0"
  }
}
