{
  "name": "r-factor",
  "version": "0.0.1",
  "description": "React refactoring tools for Sublime 3",
  "main": "index.js",
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC BY-NC-ND 4.0",
  "scripts": {
    "lint": "eslint config . --ext .js",
    "lint:fix": "eslint config . --ext .js --fix",
    "test": "jest --no-cache --coverage",
    "test:watch": "jest --no-cache --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ]
  },
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.46",
    "@babel/traverse": "^7.0.0-beta.46",
    "babylon": "^7.0.0-beta.30",
    "get-stdin": "^6.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^22.4.3"
  }
}
