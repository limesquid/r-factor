{
  "name": "r-factor",
  "version": "0.0.1",
  "description": "React refactoring tools for Sublime 3",
  "main": "index.js",
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil.adam.mielnik@gmail.com",
    "url": "http://kamilmielnik.com/"
  },
  "license": "CC BY-NC-ND 4.0",
  "scripts": {
    "analyze-bundle": "better-npm-run analyze-bundle",
    "build": "better-npm-run build",
    "lint": "eslint config . --ext .js",
    "lint:fix": "eslint config . --ext .js --fix",
    "test": "jest --no-cache --coverage",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --no-cache --watch",
    "test:debug:windows": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache --watch",
    "test:watch": "jest --no-cache --watch"
  },
  "betterScripts": {
    "analyze-bundle": {
      "command": "webpack",
      "env": {
        "ANALYZE_BUNDLE": "true",
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ]
  },
  "dependencies": {
    "@babel/generator": "7.0.0-beta.51",
    "@babel/parser": "7.0.0-beta.51",
    "@babel/traverse": "7.0.0-beta.51",
    "@babel/types": "7.0.0-beta.51",
    "babel-eslint": "8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "get-stdin": "6.0.0",
    "lodash.difference": "4.5.0",
    "stable": "0.1.8",
    "yargs": "11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-loader": "^8.0.0-beta.4",
    "better-npm-run": "^0.1.0",
    "jest": "23.1.0",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8"
  }
}
